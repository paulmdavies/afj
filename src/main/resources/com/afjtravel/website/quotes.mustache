{{> header}}
<h2>Quotes</h2>
<table>
    <tr>
        <th>Reference</th>
        <th>Quote Status</th>
        <th>First Name</th>
        <th>Surname</th>
        <th>Company Name</th>
    </tr>
    {{#quotes}}
    <tr>
        <td>{{reference}}</td>
        <td>{{quoteStatus}}</td>
        <td>{{firstName}}</td>
        <td>{{surname}}</td>
        <td>{{companyName}}</td>
    </tr>
    {{/quotes}}
</table>
{{> footer}}